{
    "name": "multi-encode",
    "displayName": "multi-encode",
    "description": "This extention provides many encoders.",
    "version": "0.0.1",
    "publisher": "wraith13",
    "engines": {
        "vscode": "^1.5.0"
    },
    "bugs": {
        "url": "https://github.com/wraith13/multi-encode/issues"
    },
    "homepage": "https://github.com/wraith13/multi-encode/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/wraith13/multi-encode.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:multi-encode.kick"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "multi-encode.kick",
            "title": "multi encode",
            "when": "editorHasSelection"
        }],
        "configuration": {
            "title": "multi-encode configuration",
            "properties": {
                "multi-encode.list": {
                    "type": "array",
                    "default": [
                        {
                            "label": "toLowerCase",
                            "description": "s => s.toLowerCase()",
                            "isPreviewable": true
                        },
                        {
                            "label": "toUpperCase",
                            "description": "s => s.toUpperCase()",
                            "isPreviewable": true
                        },
                        {
                            "label": "encodeURI",
                            "description": "s => encodeURI(s)",
                            "isPreviewable": true
                        },
                        {
                            "label": "decodeURI",
                            "description": "s => decodeURI(s)",
                            "isPreviewable": true
                        },
                        {
                            "label": "\\ escape",
                            "description": "s => JSON.stringify(s)",
                            "isPreviewable": true
                        },
                        {
                            "label": "eval",
                            "description": "s => eval(s)",
                            "isPreviewable": false
                        },
                        {
                            "label": "format JSON",
                            "description": "s => JSON.stringify(JSON.parse(s),null,4)",
                            "isPreviewable": false
                        },
                        {
                            "label": "Join lines",
                            "description": "s => s.replace(/(\\r\\n|\\r|\\n)/g, \"\")",
                            "isPreviewable": true
                        }
                    ],
                    "description": "encoder/decoder list"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}