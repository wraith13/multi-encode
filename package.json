{
    "name": "multi-encode",
    "displayName": "multi-encode",
    "description": "This extention provides many encoders.",
    "version": "0.0.1",
    "publisher": "wraith13",
    "engines": {
        "vscode": "^1.5.0"
    },
    "bugs": {
        "url": "https://github.com/wraith13/multi-encode/issues"
    },
    "homepage": "https://github.com/wraith13/multi-encode/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/wraith13/multi-encode.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:multi-encode.kick"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "multi-encode.kick",
            "title": "multi encode"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}